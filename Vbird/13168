    <script>
        Wind.ready('jquery.js', function() {
            window.setTimeout(function() {
                $('#xmGuestPrompt_footer').fadeIn();
            }, 3000);
            $('#xmGuestPrompt_close').click(function() {
                $('#xmGuestPrompt_footer').fadeOut();
            });

            var username = $('#xmGuestPrompt_username'),
                    sidebar_login_btn = $('#xmGuestPrompt_login');
            if (username.length) {
                Wind.use('draggable', 'ajaxForm', function() {
                    var password = $('#xmGuestPrompt_password');
                    $("#xmGuestPrompt_form").ajaxForm({
                        dataType: 'json',
                        beforeSubmit: function(arr, $form, options) {
                            Wind.Util.ajaxBtnDisable(sidebar_login_btn);
                            $('#J_login_tips').remove();
                        },
                        success: function(data, statusText, xhr, $form) {
                            if (data.state === 'success') {
                                if (data.message.check.url) {
                                    //验证问题
                                    Wind.Util.ajaxBtnEnable(sidebar_login_btn);
                                    $.post(data.message.check.url, function(data) {
                                        //引入所需组件并显示弹窗
                                        $('body').append(data);
                                       //获得焦点
                                        var question_wrap = $('#J_login_question_set_wrap, #J_login_question_wrap');
                                        //global.js
                                        Wind.Util.popPos(question_wrap);
                                        question_wrap.find('input:text:visible:first').focus();
                                        Wind.Util.getVerifyTemp({
                                            wrap: $('#J_verify_code')				//验证码容器
                                        });
                                    }, 'html');
                                } else {
                                    window.location.href = data.referer;
                                }
                            } else {
                                Wind.Util.ajaxBtnEnable(sidebar_login_btn);
                                $('<li id="J_login_tips" style="display:none;"><div class="tips"><div class="tips_icon_error">' + data.message + '</div></div></li>').appendTo($('#xmGuestPrompt_ul')).fadeIn(200).delay(5000).fadeOut();
                            }
                        }

                    });

                });

            }
        });

    </script>

